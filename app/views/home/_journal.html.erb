<div id='journal-page-container'>
  <div id='journal-entry'>
    <div id='journal-header'>
      <div id='journal-title'>
        <a>title here</a>
      </div>
      <div id='journal-date'>
        <a>date here</a>
      </div>
    </div>
    <div id=journal-content'>
      <% if @journal %>
        <a><%= @journal.content %></a>
      <% else %>
        <a>blank journal</a>
      <% end %>
    </div>
  </div>
</div>
